    <Color x:Key="PanelBorderColorLeft2">#E0D0DFDF</Color>
    <Color x:Key="PanelBorderColorRight1">#E0C0CFCF</Color>
    <Color x:Key="PanelBorderColorRight2">#E0B0BFBF</Color>
