    <Color x:Key="PanelDarkColor">#E0E0EFEF</Color>

    <Color x:Key="PanelBorderColorTop1">#E0FFFFFF</Color>
    <Color x:Key="PanelBorderColorTop2">#E0F0FFFF</Color>
    <Color x:Key="PanelBorderColorLeft1">#E0F0FFFF</Color>
    <Color x:Key="PanelBorderColorLeft2">#E0D0DFDF</Color>
    <Color x:Key="PanelBorderColorRight1">#E0C0CFCF</Color>
    <Color x:Key="PanelBorderColorRight2">#E0B0BFBF</Color>
    <Color x:Key="PanelBorderColorBottom1">#E0A0AFAF</Color>
